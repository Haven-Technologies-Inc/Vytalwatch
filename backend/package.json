{
  "name": "reshadx-backend",
  "version": "1.0.0",
  "description": "ReshADX Backend - Enterprise Open Banking API for Africa",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "knex migrate:latest",
    "db:migrate:make": "knex migrate:make",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "docker:build": "docker build -t reshadx-backend .",
    "docker:run": "docker run -p 3000:3000 reshadx-backend",
    "k8s:deploy": "kubectl apply -f k8s/",
    "generate:docs": "swagger-jsdoc -d swaggerDef.js src/routes/*.ts -o swagger.json"
  },
  "keywords": [
    "open-banking",
    "fintech",
    "africa",
    "api",
    "plaid-alternative"
  ],
  "author": "ReshADX",
  "license": "PROPRIETARY",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "mongodb": "^6.3.0",
    "redis": "^4.6.12",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "kafkajs": "^2.2.4",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "stripe": "^14.10.0",
    "twilio": "^4.20.0",
    "nodemailer": "^6.9.7",
    "sharp": "^0.33.1",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.3",
    "crypto-js": "^4.2.0",
    "speakeasy": "^2.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "graphql": "^16.8.1",
    "express-graphql": "^0.12.0",
    "dataloader": "^2.2.2",
    "ws": "^8.16.0",
    "socket.io": "^4.6.1",
    "@sentry/node": "^7.91.0",
    "@elastic/elasticsearch": "^8.11.0",
    "prom-client": "^15.1.0",
    "newrelic": "^11.7.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "ml-matrix": "^6.11.0",
    "natural": "^6.10.4",
    "compromise": "^14.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.5",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.0",
    "@types/multer": "^1.4.11",
    "@types/cookie-parser": "^1.4.6",
    "@types/express-session": "^1.17.10",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "prettier": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
